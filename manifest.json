{
  "manifest_version": 2,

  "name": "GIF Generator",
  "description": "This extension will capture dank videos and convert gifs",
  "version": "1.0",

  "browser_action": {
     "default_icon": {
        "16": "icons/16-uganda.png",
        "48": "icons/48-uganda.png",
        "128": "icons/128-uganda.png"
     },
     "default_popup": "popup.html"
  },
  "permissions": [
      "activeTab"
   ]
}